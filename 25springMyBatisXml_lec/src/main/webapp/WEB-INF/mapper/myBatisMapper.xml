<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
                        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="myBatisDatabase">
  <insert parameterType="com.tjoeun.beans.Mybatistbl" id="insertMember">
    <![CDATA[
      INSERT INTO MYBATISTBL VALUES(#{name}, #{id}, #{addr})
    ]]>
  </insert>

  <select resultType="com.tjoeun.beans.Mybatistbl" id="selectAll">
    <![CDATA[
      SELECT * FROM MYBATISTBL
    ]]>
  </select>
  
</mapper>                  



     